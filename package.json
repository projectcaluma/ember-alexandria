{
  "name": "ember-alexandria",
  "version": "3.0.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon",
    "ember-engine"
  ],
  "repository": "projectcaluma/ember-alexandria",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --print-name --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --print-name --aggregate-output --continue-on-error --parallel \"lint:*:fix\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "start-proxy": "ember serve --proxy=http://localhost:8000",
    "test": "npm-run-all --print-name \"lint\" \"test:*\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@ember/legacy-built-in-components": "^0.4.2",
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^2.0.5",
    "@embroider/macros": "^1.10.0",
    "@fortawesome/ember-fontawesome": "^0.4.1",
    "@fortawesome/free-regular-svg-icons": "^6.3.0",
    "@fortawesome/free-solid-svg-icons": "^6.3.0",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "animate-css-grid": "^1.5.1",
    "ember-auto-import": "^2.6.1",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-composable-helpers": "^5.0.0",
    "ember-concurrency": "^2.3.7",
    "ember-data": "^4.8.12",
    "ember-fetch": "^8.1.2",
    "ember-intl": "^5.7.2",
    "ember-localized-model": "^2.0.0",
    "ember-modifier": "^4.1.0",
    "ember-promise-helpers": "^2.0.0",
    "ember-simple-auth-oidc": "^5.0.3",
    "ember-truth-helpers": "^3.1.1",
    "ember-uikit": "^6.1.1",
    "file-saver": "^2.0.5"
  },
  "devDependencies": {
    "@adfinis-sygroup/eslint-config": "1.5.0",
    "@adfinis-sygroup/semantic-release-config": "3.4.0",
    "@babel/eslint-parser": "7.21.3",
    "@babel/plugin-proposal-decorators": "7.21.0",
    "@ember/string": "^3.0.1",
    "@ember/test-helpers": "2.9.3",
    "@embroider/test-setup": "2.1.1",
    "@faker-js/faker": "7.6.0",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli": "4.8.0",
    "ember-cli-dependency-checker": "3.3.1",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-mirage": "3.0.0-alpha.3",
    "ember-cli-sass": "11.0.1",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.2",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-engines": "0.9.0",
    "ember-load-initializers": "2.1.2",
    "ember-mirage-sauce": "1.0.0",
    "ember-qunit": "6.2.0",
    "ember-resolver": "10.0.0",
    "ember-sinon-qunit": "7.1.4",
    "ember-source": "4.8.2",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "5.7.1",
    "ember-template-lint-plugin-prettier": "4.1.0",
    "ember-test-selectors": "6.0.0",
    "ember-try": "2.0.0",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-ember": "11.4.8",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-qunit": "7.3.4",
    "graphql": "16.6.0",
    "loader.js": "4.7.0",
    "miragejs": "^0.1.46",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.6",
    "qunit": "2.19.4",
    "qunit-dom": "2.0.0",
    "sass": "1.59.3",
    "sinon": "^15.0.2",
    "webpack": "5.76.3"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "peerDependencies": {
    "ember-engines": "0.8.23"
  },
  "release": {
    "extends": "@adfinis-sygroup/semantic-release-config"
  }
}
