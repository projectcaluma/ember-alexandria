{
  "name": "ember-alexandria",
  "version": "4.7.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon",
    "ember-engine"
  ],
  "repository": "projectcaluma/ember-alexandria",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.scss\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "start-proxy": "ember serve --proxy=http://localhost:8000",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@babel/core": "^7.23.5",
    "@ember/legacy-built-in-components": "^0.5.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^2.1.0",
    "@embroider/macros": "^1.13.2",
    "@fortawesome/ember-fontawesome": "^2.0.0",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "animate-css-grid": "^1.5.1",
    "ember-auto-import": "^2.6.3",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-composable-helpers": "^5.0.0",
    "ember-concurrency": "^3.1.1",
    "ember-data": "^3.28.13 || ^4.12.5",
    "ember-data-resources": "^5.0.0",
    "ember-engines-router-service": "^0.5.0",
    "ember-fetch": "^8.1.2",
    "ember-flatpickr": "^4.0.0",
    "ember-intl": "^6.3.2",
    "ember-localized-model": "^3.2.1",
    "ember-modifier": "^4.1.0",
    "ember-promise-helpers": "^2.0.0",
    "ember-resources": "^6.4.0",
    "ember-simple-auth-oidc": "^6.0.0",
    "ember-truth-helpers": "^4.0.3",
    "ember-uikit": "^9.0.0",
    "file-saver": "^2.0.5",
    "luxon": "^3.4.4"
  },
  "devDependencies": {
    "@adfinis/eslint-config": "2.1.0",
    "@adfinis/semantic-release-config": "4.1.0",
    "@babel/eslint-parser": "7.23.3",
    "@babel/plugin-proposal-decorators": "7.23.5",
    "@ember/test-helpers": "3.2.1",
    "@embroider/test-setup": "3.0.3",
    "@faker-js/faker": "8.3.1",
    "@fortawesome/fontawesome-svg-core": "6.5.1",
    "broccoli-asset-rev": "3.0.0",
    "concurrently": "8.2.2",
    "ember-cli": "5.4.1",
    "ember-cli-clean-css": "3.0.0",
    "ember-cli-dependency-checker": "3.3.2",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-mirage": "3.0.2",
    "ember-cli-sass": "11.0.1",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.2",
    "ember-engines": "0.9.0",
    "ember-load-initializers": "2.1.2",
    "ember-qunit": "8.0.2",
    "ember-resolver": "11.0.1",
    "ember-simple-auth": "6.0.0",
    "ember-sinon-qunit": "7.4.0",
    "ember-source": "5.4.0",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "5.13.0",
    "ember-template-lint-plugin-prettier": "5.0.0",
    "ember-test-selectors": "6.0.0",
    "ember-try": "3.0.0",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-ember": "11.11.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-n": "16.3.1",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-qunit": "8.0.1",
    "graphql": "16.8.1",
    "loader.js": "4.7.0",
    "miragejs": "0.1.48",
    "prettier": "3.1.0",
    "qunit": "2.20.0",
    "qunit-dom": "3.0.0",
    "sass": "1.69.5",
    "semantic-release": "22.0.9",
    "sinon": "17.0.1",
    "stylelint": "15.11.0",
    "stylelint-config-standard-scss": "11.1.0",
    "stylelint-prettier": "4.1.0",
    "stylelint-scss": "5.3.1",
    "uikit": "3.17.11",
    "webpack": "5.89.0"
  },
  "//": [
    "@ember-data/graph@4.12.5 needs @ember-data/store@5.3.0 as peer",
    "dependency which causes issues (and doesn't make any sense) for embroider",
    "builds used in the test scenarios:",
    "https://github.com/embroider-build/embroider/blob/main/docs/peer-dependency-resolution-issues.md"
  ],
  "pnpm": {
    "overrides": {
      "@ember-data/store": "4.12.5"
    }
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "peerDependencies": {
    "ember-engines": "^0.9.0",
    "ember-source": "^4.0.0 || ^5.0.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "packageManager": "pnpm@8.9.2",
  "release": {
    "extends": "@adfinis/semantic-release-config"
  }
}
