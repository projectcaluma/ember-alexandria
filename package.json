{
  "name": "ember-alexandria",
  "version": "4.3.0",
  "description": "The default blueprint for ember-cli addons.",
  "keywords": [
    "ember-addon",
    "ember-engine"
  ],
  "repository": "projectcaluma/ember-alexandria",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.scss\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "start-proxy": "ember serve --proxy=http://localhost:8000",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@ember/legacy-built-in-components": "^0.4.2",
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^2.1.0",
    "@embroider/macros": "^1.13.1",
    "@fortawesome/ember-fontawesome": "^1.0.3",
    "@fortawesome/free-regular-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "animate-css-grid": "^1.5.1",
    "ember-auto-import": "^2.6.3",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-composable-helpers": "^5.0.0",
    "ember-concurrency": "^3.1.1",
    "ember-data": "^3.28.13 || ^4.12.3",
    "ember-data-resources": "^5.0.0",
    "ember-engines-router-service": "^0.3.0",
    "ember-fetch": "^8.1.2",
    "ember-intl": "^5.7.2",
    "ember-localized-model": "^3.2.0",
    "ember-modifier": "^4.1.0",
    "ember-promise-helpers": "^2.0.0",
    "ember-resources": "^6.4.0",
    "ember-simple-auth-oidc": "^5.1.0",
    "ember-truth-helpers": "^3.1.1",
    "ember-uikit": "^8.0.0",
    "file-saver": "^2.0.5"
  },
  "devDependencies": {
    "@adfinis/eslint-config": "2.0.0",
    "@adfinis/semantic-release-config": "4.0.0",
    "@babel/eslint-parser": "7.22.15",
    "@babel/plugin-proposal-decorators": "7.22.15",
    "@ember/string": "3.1.1",
    "@ember/test-helpers": "3.2.0",
    "@embroider/test-setup": "3.0.1",
    "@faker-js/faker": "8.1.0",
    "broccoli-asset-rev": "3.0.0",
    "concurrently": "8.2.1",
    "ember-cli": "4.12.1",
    "ember-cli-dependency-checker": "3.3.2",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-mirage": "3.0.0-alpha.3",
    "ember-cli-sass": "11.0.1",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.2",
    "ember-engines": "0.9.0",
    "ember-load-initializers": "2.1.2",
    "ember-mirage-sauce": "1.0.0",
    "ember-qunit": "7.0.0",
    "ember-resolver": "11.0.1",
    "ember-sinon-qunit": "7.1.4",
    "ember-source": "4.12.3",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "5.11.2",
    "ember-template-lint-plugin-prettier": "5.0.0",
    "ember-test-selectors": "6.0.0",
    "ember-try": "3.0.0",
    "eslint": "8.49.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-ember": "11.11.1",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-n": "16.1.0",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-qunit": "8.0.0",
    "graphql": "16.8.0",
    "loader.js": "4.7.0",
    "miragejs": "0.1.47",
    "prettier": "3.0.3",
    "qunit": "2.19.4",
    "qunit-dom": "2.0.0",
    "sass": "1.67.0",
    "sinon": "15.2.0",
    "stylelint": "15.10.3",
    "stylelint-config-standard-scss": "11.0.0",
    "stylelint-prettier": "4.0.2",
    "stylelint-scss": "5.2.0",
    "webpack": "5.88.2"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "peerDependencies": {
    "ember-engines": "^0.9.0",
    "ember-source": "^4.0.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "release": {
    "extends": "@adfinis/semantic-release-config"
  }
}
